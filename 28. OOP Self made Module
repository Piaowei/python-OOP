def triangle_area(b,h):
    print(f"Area of triangle is : {0.5*b*h}")

def rectangle_area(a,b):
    print(f"Area of rectangle is : {a*b}")
def add_num (a,b) :
    print(f"Addition of 2 number is  {a+b}")

